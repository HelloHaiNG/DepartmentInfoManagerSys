<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="department" extends="struts-default">
		<action name="department_*" class="departmentAction" method="{1}">
		    <result name="toAddPage">/jsp/department/view/add.jsp</result>
			<result name="add">/jsp/success.jsp</result>
			<result name="list">/jsp/department/view/departmentList.jsp</result>
			<result name="deleteByDname">/jsp/success.jsp</result>
			<result name="error">/jsp/error.jsp</result>
		</action>
	</package>
</struts> 
