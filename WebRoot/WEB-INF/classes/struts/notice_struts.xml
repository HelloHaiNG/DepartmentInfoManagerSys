<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="notice" extends="struts-default">
		<action name="notice_*" class="noticeAction" method="{1}">
		    <result name="toAddPage">/jsp/notice/add.jsp</result>
			<result name="add">/jsp/success.jsp</result>
			<result name="all">/jsp/notice/list.jsp</result>
			<result name="deleteByNid">/jsp/success.jsp</result>
			<result name="delete">/jsp/success.jsp</result>
			<result name="error">/jsp/error.jsp</result>
		</action>
	</package>
</struts> 
